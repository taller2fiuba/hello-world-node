{
  "name": "nodejs-edjus-demo",
  "version": "1.0.0",
  "description": "nodejs image demo",
  "author": "Edson Justo",
  "license": "MIT",
  "main": "app.js",
  "scripts": {
    "pretest": "eslint --ignore-path .gitignore .",
    "fix-lint": "eslint --fix --ignore-path .gitignore .",
    "test": "env-cmd -f ./config/test.env istanbul cover _mocha",
    "coveralls": "istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "start": "node app.js",
    "dev": "env-cmd -f ./config/dev.env nodemon app.js"
  },
  "keywords": [
    "nodejs",
    "bootstrap",
    "express"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "express": "^4.16.4",
    "mongoose": "^5.9.10"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.18.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.13",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "istanbul": "^0.4.5",
    "mocha": "^7.1.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nock": "^12.0.3",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5"
  }
}
